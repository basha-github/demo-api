package com.example.demo;

public class Employee {
	
	
	private int id;
	private String name;
	private int sal;
	private String org;
	
	public Employee() {
	
	}

	public Employee(int id, String name, int sal, String org) {
		super();
		this.id = id;
		this.name = name;
		this.sal = sal;
		this.org = org;
	}
	
	
	
	
	

}
